-- AÃ±adir el contorno de texto (TextStroke) con mayor visibilidad y color mÃ¡s brillante
titleLabel.TextStrokeTransparency = 0.1  -- MÃ¡s visible y grueso
titleLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)  -- Contorno negro para darle un toque mÃ¡s "crudo"

-- AnimaciÃ³n de cambio de color (cÃ³digo verde â†’ verde neon â†’ rojo neÃ³n)
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)

-- -- Inicializa el primer color de la animación (rojo neón)
local goal = {TextColor3 = Color3.fromRGB(255, 0, 0)}  -- Rojo neón
local tween = tweenService:Create(titleLabel, tweenInfo, goal)
tween:Play()


-- Cambiar a color rojo neÃ³n despuÃ©s de que la animaciÃ³n verde termine
local TweenService = game:GetService("TweenService")

local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
local goalGreen = {TextColor3 = Color3.fromRGB(0, 255, 0)} -- Verde neón
local goalRed = {TextColor3 = Color3.fromRGB(255, 0, 0)} -- Rojo neón

local function animateColor()
	local tweenToRed = TweenService:Create(titleLabel, tweenInfo, goalRed)
	tweenToRed:Play()
	tweenToRed.Completed:Connect(function()
		local tweenToGreen = TweenService:Create(titleLabel, tweenInfo, goalGreen)
		tweenToGreen:Play()
		tweenToGreen.Completed:Connect(function()
			animateColor() -- Vuelve a comenzar
		end)
	end)
end

animateColor()



