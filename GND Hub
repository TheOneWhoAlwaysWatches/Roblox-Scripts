-- Services
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- Verified users
local verifiedUsers = { "iamrobloxbrotherr" }

-- Welcome notification
pcall(function()
    StarterGui:SetCore("SendNotification", {
        Title = "GND HUB",
        Text = "(✅Welcome)",
        Duration = 15,
        Icon = "rbxthumb://type=Asset&id=111505420074613&w=150&h=150"
    })
end)

-- Kick unverified users
if not table.find(verifiedUsers, player.Name) then
    player:Kick("Try more kid u ain't getting Nothing")
    return
end

