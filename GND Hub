-- Tween para borde
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)

local colorSequence = {
	{Color = Color3.fromRGB(255, 0, 0)},
	{Color = Color3.fromRGB(100, 0, 0)}
}

local currentIndex = 1
local function playNextTween()
	currentIndex = 3 - currentIndex
	local goal = colorSequence[currentIndex]
	local tween = tweenService:Create(uiStroke, tweenInfo, goal)
	tween:Play()
	tween.Completed:Connect(playNextTween)
end

playNextTween()

-- Efecto glow/parpadeo rojo para el texto
local function animateTextPulse()
	while true do
		tweenService:Create(toggleButton, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
			TextColor3 = Color3.fromRGB(255, 80, 80)
		}):Play()
		wait(0.4)
		tweenService:Create(toggleButton, TweenInfo.new(0.4, Enum.EasingStyle.Linear), {
			TextColor3 = Color3.fromRGB(255, 0, 0)
		}):Play()
		wait(0.4)
	end
end

task.spawn(animateTextPulse)

-- Alternar visibilidad y texto
